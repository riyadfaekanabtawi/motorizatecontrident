 <div class="row">
        <div class="col-md-12">

          <!-- Profile Image -->
          <div class="box box-primary">
            <div class="box-body box-profile">
              <img class="profile-user-img img-responsive img-circle" src="<%= @store.photo.url(:small) %>" alt="Sucursal">

              <h3 class="profile-username text-center">Sucursal: <%= @store.nombre %><br> Ciudad: <%= @store.ciudad %></h3>

     
              <ul class="list-group list-group-unbordered">
                <li class="list-group-item">
                  <b>Direccion: </b> <a class="pull-right"><%= @store.direccion %></a>
                </li>

              </ul>

            </div>
          
          </div>
       
        </div>
  

  
     
      </div>
      <!-- /.row -->


